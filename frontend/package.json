{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "VITE_ANALYZE_BUNDLE=true vite build",
    "preview": "vite preview",
    "lint": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "perf:verify": "vitest run src/test/performanceChecks.test.ts",
    "perf:lhci": "npx --yes @lhci/cli@0.13.x autorun --config=./lighthouserc.json",
    "playwright": "playwright",
    "format": "biome format --write .",
    "format:check": "biome check --formatter-enabled=true --linter-enabled=false .",
    "biome:ci": "biome ci .",
    "generate:api": "npm run generate:api-types && npm run generate:zod-schemas",
    "generate:api-types": "openapi-ts -i ../openapi/openapi.yaml -o src/types -c @hey-api/typescript",
    "generate:zod-schemas": "openapi-zod-client ../openapi/openapi.yaml -o src/schemas/api.ts --export-schemas --with-docs --strict-objects",
    "measure:bundle": "npm run build && node ../.kiro/specs/critical-serena-refactoring/scripts/dist/collect-metrics.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "events": "^3.3.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-router-dom": "^7.9.2",
    "recharts": "^3.3.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@hey-api/openapi-ts": "^0.84.3",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@zodios/core": "^10.9.6",
    "axios-mock-adapter": "^2.1.0",
    "jsdom": "^27.0.0",
    "knip": "^5.66.3",
    "msw": "^2.11.3",
    "openapi-zod-client": "^1.18.3",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "ultracite": "^5.4.4",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "zod": "^3.25.76"
  }
}
