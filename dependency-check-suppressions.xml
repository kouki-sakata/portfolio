<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Spring Boot関連の既知の誤検出を抑制 -->
    <suppress>
        <notes><![CDATA[
            Spring Boot starter dependencies are maintained by the Spring team
            and follow security best practices.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>

    <!-- MySQL Connector関連の既知の誤検出を抑制 -->
    <suppress>
        <notes><![CDATA[
            MySQL Connector/J false positives for unrelated components
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/mysql/mysql\-connector\-java.*$</packageUrl>
        <cve>CVE-2019-2692</cve>
    </suppress>

    <!-- Logback関連の既知の誤検出を抑制 -->
    <suppress>
        <notes><![CDATA[
            Logback vulnerability not applicable to our usage
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/ch\.qos\.logback/.*$</packageUrl>
        <cve>CVE-2017-5929</cve>
    </suppress>

    <!-- テスト依存関係の抑制 -->
    <suppress>
        <notes><![CDATA[
            Test dependencies are not deployed to production
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\..*$</packageUrl>
        <cve>.*</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
            Mockito test framework vulnerabilities not applicable in test context
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mockito/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
</suppressions>